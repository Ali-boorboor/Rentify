{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@loginRegister/*": ["./src/app/(Login-Register)/*"],
      "@otpVerify/*": [
        "./src/app/(Login-Register)/login-register/(Otp-Verify)/*"
      ],
      "@home/*": ["./src/app/(Home-Page)/*"],
      "@properties/*": ["./src/app/(Properties-Page)/*"],
      "@singleProperty/*": [
        "./src/app/(Properties-Page)/properties/(Single-Property-Page)/*"
      ],
      "@contactUs/*": ["./src/app/(Contact-Us)/*"],
      "@userPanel/*": ["./src/app/(User-Panel)/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
